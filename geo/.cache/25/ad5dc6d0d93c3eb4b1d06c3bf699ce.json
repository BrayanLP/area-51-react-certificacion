iginal":{"line":51,"column":16}},{"generated":{"line":107,"column":39},"source":"src/components/UserComponent.js","original":{"line":51,"column":17}},{"generated":{"line":107,"column":56},"source":"src/components/UserComponent.js","original":{"line":51,"column":16}},{"generated":{"line":108,"column":0},"source":"src/components/UserComponent.js","original":{"line":51,"column":26}},{"generated":{"line":108,"column":8},"source":"src/components/UserComponent.js","original":{"line":51,"column":26}},{"generated":{"line":108,"column":12},"source":"src/components/UserComponent.js","original":{"line":51,"column":30}},{"name":"currentUsers","generated":{"line":108,"column":14},"source":"src/components/UserComponent.js","original":{"line":51,"column":32}},{"generated":{"line":109,"column":0},"source":"src/components/UserComponent.js","original":{"line":51,"column":16}},{"generated":{"line":109,"column":8},"source":"src/components/UserComponent.js","original":{"line":49,"column":12}},{"generated":{"line":109,"column":9},"source":"src/components/UserComponent.js","original":{"line":48,"column":8}},{"generated":{"line":110,"column":0},"source":"src/components/UserComponent.js","original":{"line":55,"column":5}},{"name":"Component","generated":{"line":114,"column":2},"source":"src/components/UserComponent.js","original":{"line":8,"column":28}},{"name":"UserComponent","generated":{"line":116,"column":15},"source":"src/components/UserComponent.js","original":{"line":57,"column":15}}],"sources":{"src/components/UserComponent.js":"import React, { Component } from 'react'\nimport User from './User'\nimport UserList from './UserList'\nimport SearchUser from './SearchUser'\nimport fuzzysearch from 'fuzzysearch'\n\nconst API = \"https://randomuser.me/api/?results=50\"\nclass UserComponent extends Component {\n    constructor(props) {\n        super(props)\n        this.state = {\n            users: [],\n            data: [],\n            filterUsers: []\n        }\n        this.handleFilter = this.handleFilter.bind(this)\n    }\n    componentDidMount() {\n        fetch(API)\n            .then(res => res.json())\n            .then(data => {\n                const { results } = data\n                this.setState({ data: results, users: results })\n            })\n    }\n    handleFilter(value) {\n        event.preventDefault()\n        // const { target } = event\n        console.log(value)\n        const { users } = this.state\n        const filterUsers = users.filter(({ name, email }) => {\n            const { first, last } = name\n            return (\n                fuzzysearch(value.toLowerCase(), first.toLowerCase()) ||\n                fuzzysearch(value.toLowerCase(), last.toLowerCase()) ||\n                fuzzysearch(value.toLowerCase(), email.toLowerCase())\n            )\n        })\n        this.setState({\n            filterUsers\n        })\n\n    }\n\n    render() {\n        const { users, filterUsers } = this.state\n        const currentUsers = filterUsers.length ? filterUsers : users\n        return (\n            <>\n                <SearchUser handleFilter={this.handleFilter}></SearchUser>\n                <UserList data={currentUsers}></UserList>\n            </>\n\n        )\n    }\n}\nexport default UserComponent"},"lineCount":null}},"error":null,"hash":"7e5201d4d1cfaf9aabdea7b1b61daf50","cacheData":{"env":{}}}